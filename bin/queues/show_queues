#!/usr/bin/env python
import sys
sys.path.append('/opt/oplog/conf')
import oplog
import opdb
opdb = opdb.opdb()
sql = "select * from queues"
result = opdb.select(sql)
for x in range(0,len(result)):
	print "Id: %s,\tDatabase Name: %s,\tDatabase Title: %s" % (result[x][0], result[x][1], result[x][2])
opdb.close()
