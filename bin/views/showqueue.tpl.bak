<html>
%if res[0][1] > 1:
	<a href="/oplog">Queues</a> |<p>
%end
<h1>{{res[0][0]}}</h1>
<a href="/oplog/{{res[0][0]}}"> Previous </a><p>
%for i in res[1]['hits']['hits']:
	<h3>{{i['_source']['subject']}}</h3>
	<b>From:</b> {{i['_source']['from']}}<br>
	<b>Sent:</b> {{i['_source']['sent']}}<br>
	<b>Received:</b> {{i['_source']['submitted']}}<p>
	{{i['_source']['body']}}<p>
	--------------------------------------------------------------------<p>
%end
</html>
